<?xml version="1.0" encoding="UTF-8"?>
<manifest>
	<remote name="github" fetch="ssh://git@github.com" />
	<default revision="master" remote="github" />

	<!-- manifest -->

	<project name="Wonko7/conf-manifest"
	         path="manifest" remote="github" />

	<!-- main config modules -->

	<project name="Wonko7/conf-vim"
	         path="vim/" remote="github" />

	<project name="Wonko7/conf-zsh"
	         path="zsh/" remote="github" />

	<project name="Wonko7/conf-git"
	         path="git/" remote="github" />

	<project name="Wonko7/conf-xmonad"
	         path="xmonad/" remote="github" />

	<project name="Wonko7/conf-misc"
	         path="misc/" remote="github" />

	<!-- zsh plugins -->

	<project name="zsh-users/zsh-completions"
		 path="zsh/completions" remote="github" />

	<project name="zsh-users/zsh-syntax-highlighting"
		 path="zsh/syntax-highlighting" remote="github" />


	<!-- vim plugins -->

</manifest>
